<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.FlowPane?>
<?import java.net.URL?>

<FlowPane  fx:id="root" fx:controller="FourButtons" xmlns:fx="http://javafx.com/fxml">
    <stylesheets>
        <URL value="@Main.css" />   
    </stylesheets>
    <children>
        <Button fx:id="redButton" onAction="#handleRed">Red</Button>
        <Button fx:id="blueButton" onAction="#handleBlue">Blue</Button>
        <Button fx:id="printButton" onAction="#handlePrint">Consol Print</Button>
        <Button fx:id="exitButton" onAction="#handleExit">Exit</Button>

    </children>
</FlowPane>